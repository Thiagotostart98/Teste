n1 = int(0)
n2 = int(1)
n3 = int(0)

num = int(input("Digite um número:"))

while num > n3:
    n3 = n1 + n2
    n1 = n2
    n2 = n3

if num == 0 | num == 1:
    print("O número " + str(num) + " está na sequência de Fibonacci.")


elif num == n3:
    print("O número " + str(num) + " está na Sequência de Fibonacci.")

else:
    print("O número não está presente na Sequência de Fibonacci.")
